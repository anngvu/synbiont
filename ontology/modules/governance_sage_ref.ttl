sagegov:RestrictedLimitedProfile a skos:Concept, sagegov:AccessProfile ;
  skos:prefLabel "Restricted/ Limited" ;
  sagegov:accessLevel sagegov:RestrictedLimited ;
  sagegov:accessLevelNote "Low-risk de-identified data whose usage is limited by contractual clauses set by data contributors" ;
  dct:description "De-identified Data whose usage is limited by contractual clause" ;
  sagegov:example "Data whose usage is limited based on:\n\n -  Area of research\n -  Non-profit use\n -  Non-commercial use\n -  Biomedical research (no ancestry)\n -  Methods development\n -  Embargo (publication/use)\n -  Return of results" ;
  sagegov:downloadable true ;
  sagegov:redistribution false ;
  sagegov:affiliationRequirement "Optional" ;
  sagegov:synapseAccountRequirement true ;
  sagegov:humanSubjectsTraining true ;
  sagegov:dataAccessRequest false ;
  sagegov:dataUseCertificate false ;
  sagegov:researchObjectiveRequirement false ;
  sagegov:irbApproval false ;
  sagegov:securityRequirements "No" ;
