sagegov:PersonallyIdentifiableInformation a skos:Concept, sagegov:AccessProfile ;
  skos:prefLabel "Personally identifiable Information" ;
  sagegov:identifiabilityRisk sagegov:HighIdentifiabilityRisk ;
  sagegov:accessLevel sagegov:Enclave ;
  sagegov:accessLevelNote "sensitive data that, when combined with easily accessible public information, carries a significant risk of revealing the identity of specific individuals. Misuse of this data could potentially cause harm to those individuals or groups" ;
  dct:description "Data that directly identifies an individual (geolocation, family pedigree)" ;
  sagegov:example "1.  Next Gen Sequencing > 50 genes\n2.  3D imaging\n3.  Geolocation \n4.  Family pedigree\n5.  Voice recordings" ;
  sagegov:downloadable false ;
  sagegov:redistribution false ;
