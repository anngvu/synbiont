sagegov:OmicsLargeScaleCoded a skos:Concept, sagegov:AccessProfile ;
  skos:prefLabel "Omics Large scale, coded" ;
  sagegov:identifiabilityRisk sagegov:SomeIdentifiabilityRisk ;
  sagegov:accessLevel sagegov:Controlled ;
  sagegov:accessLevelNote "Data that could theoretically be used to identify a unique individual and could cause more than minimal risk of harm if misused" ;
  dct:description "includes raw genomics,  metabolomics, transcriptomics, and proteomics." ;
  sagegov:example "1. Sequencing  (raw, individual level)\n -  WGS\n -  WES\n -  RNAseq, scRNAseq, snRNAseq\n -  CHIPseq\n -  ATACseq, snATACseq\n -  Hi-C, HiChip\n -  Next Gen sequencing  (<50 genes)\n2. Proteomics (raw)\n3. High content SNP array, raw & imputed\n4. Methylation array\n5. Whole transcriptome \n6. Genomic summary statistics from GWAS\n7. Imaging (raw)\n -  MRI, CT scan" ;
  sagegov:downloadable true ;
  sagegov:redistribution false ;
  sagegov:affiliationRequirement true ;
  sagegov:synapseAccountRequirement true ;
  sagegov:humanSubjectsTraining true ;
  sagegov:dataAccessRequest true ;
  sagegov:dataUseCertificate true ;
  sagegov:researchObjectiveRequirement true ;
  sagegov:irbApproval false ;
  sagegov:securityRequirements "NIST-800-171 or ISO27001 recommended" ;
  sagegov:approvalProcess "DAC \n(human review)" ;
  dct:source "reference/DataTypes-brief-Sept2025.xlsx" .

