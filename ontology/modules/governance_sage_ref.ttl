@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix sagegov: <https://synapse.org/synbiont/governance/> .

sagegov:AccessProfile a owl:Class ;
  rdfs:label "Sage governance access profile" ;
  rdfs:comment "Profiles derived from the Sage governance reference spreadsheet." .

sagegov:Data a owl:Class ;
  rdfs:label "Sage data type" ;
  rdfs:comment "Data classifications referenced in governance policies." .

sagegov:AccessLevel a owl:Class ;
  rdfs:label "Access level" ;
  rdfs:comment "Permitted usage tiers for Synapse data." .

sagegov:IdentifiabilityRisk a owl:Class ;
  rdfs:label "Identifiability risk" ;
  rdfs:comment "Relative likelihood that data could be used to re-identify individuals." .

sagegov:AccessProfileRule a owl:Class ;
  rdfs:label "Governance rule" ;
  rdfs:comment "Helper class for constraints referenced inside the governance module." ;
  rdfs:isDefinedBy sagegov:AccessProfile .

sagegov:AnonymousOrOpen a skos:Concept, sagegov:AccessLevel ;  skos:prefLabel "Anonymous / Open" ;  rdfs:comment "Data is usable without registration or affiliation." .
sagegov:Registered a skos:Concept, sagegov:AccessLevel ;  skos:prefLabel "Registered" ;  rdfs:comment "Data usage requires a Synapse account but not additional governance approvals." .
sagegov:RestrictedLimited a skos:Concept, sagegov:AccessLevel ;  skos:prefLabel "Restricted / Limited" ;  rdfs:comment "Data usage limited by contributor-defined contract terms." .
sagegov:Controlled a skos:Concept, sagegov:AccessLevel ;  skos:prefLabel "Controlled" ;  rdfs:comment "Data potentially re-identifiable and subject to access review." .
sagegov:Enclave a skos:Concept, sagegov:AccessLevel ;  skos:prefLabel "Enclave" ;  rdfs:comment "Sensitive data that must remain inside a secure compute enclave." .

sagegov:LowIdentifiabilityRisk a skos:Concept, sagegov:IdentifiabilityRisk ;  skos:prefLabel "Low" ;  rdfs:comment "Data has low likelihood of re-identification." .
sagegov:SomeIdentifiabilityRisk a skos:Concept, sagegov:IdentifiabilityRisk ;  skos:prefLabel "Some risks" ;  rdfs:comment "Data could, in certain contexts, be used to re-identify individuals." .
sagegov:HighIdentifiabilityRisk a skos:Concept, sagegov:IdentifiabilityRisk ;  skos:prefLabel "High" ;  rdfs:comment "Data is likely to re-identify individuals if misused." .

sagegov:Synthetic a skos:Concept, sagegov:Data ;
  skos:prefLabel "Synthetic" ;
  sagegov:accessLevel sagegov:AnonymousOrOpen ;
  sagegov:accessLevelNote "Data that cannot be used alone or with other data to identify a unique individiual" ;
  dct:description "Computational data derivative that statistically resembles the original data" ;
  sagegov:example "1. Aggregate/summary-level data\n2. Genomic Summary Results\n - Networks\n - Differential expression\n - eQTL analysis\n - GWAS catalog\n3. Minimal unidentifiable individual-level information\nDe-identified ID\n - Age (90+ censored) and Sex\n - Diagnosis (limited, common)\n4. Study & file annotation\n5. Non-human data\n6. Synthetic or anonymized data \n7. Synapse users public profile \n8. Public Wikis and dashboards\n9. Intended data use statements" ;
  sagegov:downloadable true ;
  sagegov:redistribution true ;
  sagegov:affiliationRequirement false ;
  sagegov:synapseAccountRequirement false ;
  sagegov:humanSubjectsTraining false ;
  sagegov:dataAccessRequest false ;
  sagegov:dataUseCertificate false ;
  sagegov:researchObjectiveRequirement false ;
  sagegov:irbApproval false ;
  sagegov:securityRequirements "No" ;
  sagegov:approvalProcess "No" ;
  dct:source "reference/DataTypes-brief-Sept2025.xlsx" .

sagegov:Anonymized a skos:Concept, sagegov:Data ;
  skos:prefLabel "Anonymized" ;
  dct:description "Data that cannot be associated to a unique individiual" ;
  sagegov:downloadable true ;
  sagegov:redistribution true ;
  sagegov:affiliationRequirement false ;
  sagegov:synapseAccountRequirement false ;
  sagegov:humanSubjectsTraining false ;
  sagegov:dataAccessRequest false ;
  sagegov:dataUseCertificate false ;
  sagegov:researchObjectiveRequirement false ;
  sagegov:irbApproval false ;
  sagegov:securityRequirements "No" ;
  sagegov:approvalProcess "No" ;
  sagegov:accessLevel sagegov:AnonymousOrOpen ;
  dct:source "reference/DataTypes-brief-Sept2025.xlsx" .

sagegov:HIPAASafeHarbor a skos:Concept, sagegov:AccessProfile, sagegov:Data ;
  skos:prefLabel "HIPAA Safe Harbor" ;
  sagegov:identifiabilityRisk sagegov:LowIdentifiabilityRisk ;
  sagegov:accessLevel sagegov:Registered ;
  sagegov:accessLevelNote "De-identified data that cannot be readily used to identify a unique individual and poses minimal risk of harm if misused" ;
  dct:description "Data stripped of 18 direct identifiers per HIPAA rules" ;
  sagegov:example "1. Individual data/metadata*\n -  Data/specimen source\n -  Age of onset, death, at visits (90+ censored)\n -  Test summary scores\n -  Post-mortem intervals\n -  Normalized values\n2. Individual summary data\n -  Gene expression counts \n -  Peak calls, loop calls, HiC interaction matrix\n3. Proteomics (processed)\n4. Metabolomics\n5. Probe-based gene expression microarrays\n6. Imaging (processed)\n7. Tumor / somatic-only data (tumor-normal pair)" ;
  sagegov:downloadable true ;
  sagegov:redistribution true ;
  sagegov:affiliationRequirement false ;
  sagegov:synapseAccountRequirement true ;
  sagegov:humanSubjectsTraining true ;
  sagegov:dataAccessRequest false ;
  sagegov:dataUseCertificate false ;
  sagegov:researchObjectiveRequirement false ;
  sagegov:irbApproval false ;
  sagegov:securityRequirements "No" ;
  sagegov:approvalProcess "Synapse account" ;
  dct:source "reference/DataTypes-brief-Sept2025.xlsx" .

sagegov:HIPAAExpertDeIdentified a skos:Concept, sagegov:AccessProfile ;
  skos:prefLabel "HIPAA Expert de-identified" ;
  dct:description "Data that an expert in statistical and scientific method has determined presents a very small risk to be used to identify an invididual" ;
  sagegov:downloadable true ;
