  sagegov:researchObjectiveRequirement false ;
  sagegov:irbApproval false ;
  sagegov:securityRequirements "No" ;
  sagegov:approvalProcess "Synapse account" ;
  dct:source "reference/DataTypes-brief-Sept2025.xlsx" .

sagegov:PseudonymizedGDPR a skos:Concept, sagegov:AccessProfile ;
  skos:prefLabel "Pseudonymized (GDPR)" ;
  dct:description "Data where individual identifiers have been replaced by a code or pseudo (false) identifier" ;
  sagegov:downloadable true ;
  sagegov:redistribution true ;
  sagegov:affiliationRequirement false ;
  sagegov:synapseAccountRequirement true ;
  sagegov:humanSubjectsTraining true ;
  sagegov:dataAccessRequest false ;
  sagegov:dataUseCertificate false ;
  sagegov:researchObjectiveRequirement false ;
